<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".Setting.ChildInfoChangeFragment"
    android:background="#FFFFFF">

    <TextView
        android:id="@+id/tv_birth_name_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="태명"
        android:textSize="16sp"
        android:textColor="#C5C5C5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="16.97dp"
        android:layout_marginTop="74.55dp"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="314dp"
        />

    <EditText
        android:id="@+id/et_birth_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="태명을 입력하세요."
        android:background="@null"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@id/tv_birth_name_text"
        android:layout_marginStart="13.3dp"
        app:layout_constraintTop_toBottomOf="@id/tv_birth_name_text"
        android:layout_marginTop="18.86dp"
        android:gravity="left"/>
    <!--<TextView
        android:id="@+id/tv_birth_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="아꺵이"
        android:background="@null"
        android:textSize="18sp"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@id/tv_birth_name_text"
        android:layout_marginStart="13.3dp"
        app:layout_constraintTop_toBottomOf="@id/tv_birth_name_text"
        android:layout_marginTop="18.86dp"
        android:gravity="left"
        android:visibility="gone"/>-->

    <ImageView
        android:id="@+id/btn_name_cancle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="317.67dp"
        android:layout_marginEnd="24.33dp"
        android:src="@drawable/ic_cancle_btn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/et_birth_name" />

    <View
        android:layout_width="338dp"
        android:layout_height="0.92dp"
        android:background="#C5C5C5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btn_name_cancle"
        android:layout_marginStart="11.48dp"
        android:layout_marginEnd="11.48dp"
        android:layout_marginTop="9dp"
        />

    <TextView
        android:id="@+id/tv_due_date_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="예정일"
        android:textSize="16sp"
        android:textColor="#C5C5C5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginStart="16.97dp"
        android:layout_marginTop="192.41dp"
        android:layout_marginEnd="299.03dp"
        />


    <ImageView
        android:id="@+id/iv_ic_calendar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_calendar"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/tv_birth_date"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginStart="317.67dp"
        android:layout_marginEnd="24.33dp"
        />


    <TextView
        android:id="@+id/tv_birth_date"
        android:layout_width="wrap_content"
        android:layout_height="24dp"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@id/tv_due_date_text"
        android:layout_marginStart="13.3dp"
        app:layout_constraintTop_toBottomOf="@id/tv_due_date_text"
        android:layout_marginTop="18.86dp"

        />

    <View
        android:layout_width="338dp"
        android:layout_height="0.92dp"
        android:background="#C5C5C5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/iv_ic_calendar"
        android:layout_marginStart="11.48dp"
        android:layout_marginEnd="11.48dp"
        android:layout_marginTop="8.35dp"
        />

    <TextView
        android:id="@+id/tv_weight_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="체중"
        android:textSize="16sp"
        android:textColor="#C5C5C5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginStart="16.97dp"
        android:layout_marginTop="300.23dp"
        android:layout_marginEnd="314.03dp"
        />

    <EditText
        android:id="@+id/et_weight"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:hint="kg"
        android:background="@null"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@id/tv_weight_text"
        android:layout_marginStart="13dp"
        app:layout_constraintTop_toBottomOf="@id/tv_weight_text"
        android:layout_marginTop="23.23dp"
        />

    <ImageView
        android:id="@+id/ic_cancle_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="317.67dp"
        android:layout_marginEnd="24.33dp"
        android:src="@drawable/ic_cancle_btn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/et_weight" />

    <View
        android:id="@+id/view_weight"
        android:layout_width="338dp"
        android:layout_height="0.92dp"
        android:background="#C5C5C5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginStart="11.48dp"
        android:layout_marginEnd="11.48dp"
        app:layout_constraintTop_toBottomOf="@id/ic_cancle_btn"
        android:layout_marginTop="9dp"
        />

    <ImageView
        android:id="@+id/iv_cancle_box"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/img_cancle_box"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginStart="31dp"
        android:layout_marginTop="602.26dp"
        android:layout_marginEnd="231.82dp"
        />

    <ImageView
        android:id="@+id/iv_ok_box"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/img_ok_box"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="602.26dp"
        android:layout_marginStart="140.04dp"
        android:layout_marginEnd="31dp"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="취소"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@id/iv_cancle_box"
        app:layout_constraintTop_toTopOf="@id/iv_cancle_box"
        app:layout_constraintEnd_toEndOf="@id/iv_cancle_box"
        app:layout_constraintBottom_toBottomOf="@id/iv_cancle_box"
        android:layout_marginStart="31.59dp"
        android:layout_marginTop="14dp"
        android:layout_marginEnd="30.59dp"
        android:layout_marginBottom="14dp"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="수정완료"
        android:textSize="20sp"
        android:textColor="#FFF"
        app:layout_constraintStart_toStartOf="@id/iv_ok_box"
        app:layout_constraintTop_toTopOf="@id/iv_ok_box"
        app:layout_constraintEnd_toEndOf="@id/iv_ok_box"
        app:layout_constraintBottom_toBottomOf="@id/iv_ok_box"
        android:layout_marginStart="60.48dp"
        android:layout_marginTop="14dp"
        android:layout_marginEnd="60.48dp"
        android:layout_marginBottom="14dp"
        />




</androidx.constraintlayout.widget.ConstraintLayout>